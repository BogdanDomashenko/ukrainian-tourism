                   obj[country.getKeyByName(item.country)] = { gdp: item.internal};\n                    }\n                    else if(item.internal == \"-\") {\n                        obj[country.getKeyByName(item.country)] = { gdp: 0};\n                    }\n                }\n            }\n            \n            let resObj = {\n                targetElementID: 'svgWorldMap',\n                colorMax: \"#48185F\",\n                colorMin: \"#C1AAD3\",\n                data: {\n                    data: {\n                    gdp: {\n                        name: 'Відвідало:',\n                        format: '{0} чоловік',\n                        thousandSeparator: ',',\n                        thresholdMax: 50000,\n                        thresholdMin: 1000\n                    },\n                    },\n                    applyData: 'gdp',\n                    values: obj\n                }\n            }\n\n            return(resObj)\n        },\n    },\n    mounted () {\n\n    }\n}\n</script>"],"sourceRoot":""}]}
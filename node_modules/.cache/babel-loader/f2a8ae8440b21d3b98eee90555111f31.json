  if (window.scrollY > 50) {\n        if(!this.isScrolling) {\n          this.isScrolling = true;\n        }\n      }\n      else {\n        this.isScrolling = false;\n      }\n    }\n  },\n  mounted() {\n    \n  },\n}\n</script>\n\n<style scoped>\n\n</style>\n"],"sourceRoot":""}]}